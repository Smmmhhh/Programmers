-- 코드를 입력하세요
SELECT      CATEGORY
        -- ,   MAX(PRICE) AS MAX_PRICE
        ,   PRICE
        ,   PRODUCT_NAME
        -- ,   RANK
FROM 
(
    SELECT    CATEGORY
            , RANK() OVER(PARTITION BY CATEGORY	ORDER BY PRICE DESC) AS RANK
            , PRODUCT_NAME
            , PRICE
    FROM FOOD_PRODUCT 
    WHERE CATEGORY LIKE '%과자%' 
        OR CATEGORY LIKE '%국%' 
        OR CATEGORY LIKE '%김치%' 
        OR CATEGORY LIKE '%식용유%'
    GROUP BY CATEGORY, PRICE, PRODUCT_NAME 
)
WHERE RANK = 1
        
ORDER BY PRICE DESC

-------------------------------------------------------
SELECT    CATEGORY
        , PRICE
        , PRODUCT_NAME
FROM FOOD_PRODUCT 
WHERE (CATEGORY, PRICE) IN (SELECT CATEGORY, MAX(PRICE) FROM FOOD_PRODUCT 
                           WHERE CATEGORY IN('과자', '국', '김치', '식용유')
                           GROUP BY CATEGORY)
ORDER BY PRICE DESC