-- 코드를 입력하세요
SELECT A.NAME, A.DATETIME
FROM 
( 
    -- 입양을 가지못한 동물들만 추출하기
    SELECT I.NAME, I.DATETIME
    FROM ANIMAL_INS I
    LEFT JOIN ANIMAL_OUTS O 
     ON I.ANIMAL_ID = O.ANIMAL_ID
    WHERE I.ANIMAL_ID NOT IN (
        SELECT ANIMAL_ID FROM ANIMAL_OUTS
    )
    ORDER BY I.DATETIME
)A
WHERE ROWNUM <= 3

------------------------------
SELECT A.NAME, A.DATETIME
FROM 
( 
    -- 입양을 가지못한 동물들만 추출하기
    SELECT I.NAME, I.DATETIME
    FROM ANIMAL_INS I
    LEFT JOIN ANIMAL_OUTS O 
     ON I.ANIMAL_ID = O.ANIMAL_ID
    WHERE O.ANIMAL_ID IS NULL 
    ORDER BY I.DATETIME
)A
WHERE ROWNUM <= 3